<manifest>
	<define>
		<domain name="efreet"/>
		<request>
			<smack request="system::homedir" type="arwxt"/>
			<smack request="system::use_internet" type="r"/>
		</request>
	</define>
	<assign>
		<filesystem path="/usr/lib/efreet/efreet_desktop_cache_create" label="efreet" exec_label="efreet"/>
		<filesystem path="/usr/lib/efreet/efreet_icon_cache_create" label="efreet" exec_label="efreet"/>
	</assign>
	<request>
		<domain name="_"/>
	</request>
</manifest>
